# Discord Bot Token (von https://discord.com/developers/applications)
DISCORD_TOKEN=your_discord_bot_token_here

# Logging Level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Pfad zur SQLite-Datenbank
DATABASE_PATH=data/loretta.db

# Bot Owner Discord ID (optional - wird für Owner-Commands verwendet)
OWNER_ID=your_discord_user_id_here

# Tenor API-Schlüssel für GIF-Suche (von https://tenor.com/developer/keyregistration)
TENOR_API=your_tenor_api_key_here